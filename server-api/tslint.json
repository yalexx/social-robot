{
  "defaultSeverity": "error",
  "extends": "tslint-config-airbnb",
  "jsRules": {},
  "max-line-length": [
    false,
    200
  ],
  "rules": {
    "eofline": false,
    "variable-name": [
      true,
      "ban-keywords",
      "allow-pascal-case",
      "check-format",
      "allow-leading-underscore"
    ]
  },
  "rulesDirectory": []
}